package com.arrayPractice1.sample;

public class ArraySample {
	public void test1() {
		int[] iarr = new int[10];
		int sum = 0;
		
		for(int i=0; i<iarr.length; i++) {
			iarr[i] = (int)(Math.random() * 100) + 1;
			System.out.print(iarr[i]+" ");
		}
		System.out.println();
		for(int i=0; i<iarr.length; i++) {
			sum += iarr[i];
		}
		
		System.out.println("총합 : "+sum);
	}
	public void test2() {
		
	}
	public void test3() {
		
	}
	public void test4() {
	
	}
	public void test5() {
	
	}
}
